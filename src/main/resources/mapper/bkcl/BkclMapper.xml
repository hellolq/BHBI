<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbg.mapper.bkcl.BkclMapper">




    <select id="selectApp_dhd" resultType="Integer">

        select count(*) from app_dhd

    </select>


    <insert id="insertIntoAPP_DHD" >
        INSERT INTO
        app_dhd_test(traceid,shopid,dhd,dhrq,status,clbj)
        VALUES (#{traceId},#{shopId},#{dhd},sysdate,'0','0')
    </insert>

